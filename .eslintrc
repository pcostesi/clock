{
    "env": {
        "browser": true
    },
    "rules": {
        "sort-vars": 0,
        "quote-props": 0,
        "no-underscore-dangle": 0,
        "no-ternary": 0,
        "func-style": 0,
        "func-names": 0,
        "consistent-this": [0, "self"],
        "no-shadow-restricted-names": 0,
        "valid-jsdoc": 0,
        "no-func-assign": 0,
        "no-extra-parens": 0,

        "one-var": 1,
        "quotes": [1, "single", "avoid-escape"],
        "no-wrap-func": 1,
        "no-new-object": 1,
        "no-lonely-if": 1,
        "new-parens": 1,
        "yoda": [1, "never"],
        "no-unused-expressions": 1,
        "no-sequences": 1,
        "no-redeclare": 1,
        "no-multi-str": 1,
        "no-lone-blocks": 1,
        "no-floating-decimal": 1,
        "no-fallthrough": 1,
        "guard-for-in": 1,
        "eqeqeq": 1,
        "block-scoped-var": 1,
        "valid-typeof": 1,
        "no-unreachable": 1,
        "no-dupe-keys": 1,
        "no-constant-condition": 1,

        "wrap-regex": 2,
        "max-nested-callbacks": [2, 5],
        "space-unary-word-ops": 2,
        "space-return-throw-case": 2,
        "space-infix-ops": 2,
        "space-in-brackets": [2, "never"],
        "space-after-keywords": [2, "always"],
        "semi": 2,
        "no-space-before-semi": 2,
        "no-spaced-func": 2,
        "no-array-constructor": 2,
        "no-nested-ternary": 2,
        "new-cap": 2,
        "camelcase": 2,
        "brace-style": [2, "1tbs"],
        "strict": 2,
        "wrap-iife": [2, "inside"],
        "radix": 2,
        "no-script-url": 2,
        "no-octal": 2,
        "no-native-reassign": 2,
        "no-labels": 2,
        "no-implied-eval": 2,
        "no-eval": 2,
        "no-alert": 2,
        "dot-notation": 2,
        "default-case": 2,
        "curly": 2,
        "use-isnan": 2,
        "no-invalid-regexp": 2,
        "no-empty": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-comma-dangle": 2
    }
}
